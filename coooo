#include <stdio.h>
#include <string.h>
int main()
{
    char a[20];
    int i, l,c=0;
    printf("Enter a string:");
    scanf("%s", a);
    l = strlen(a);
    
    for(i=0;i < l ;i++)
    {
        if(a[i] != a[l-i-1])
        {
            c = 1;
            break;
   }
}
     if (c)
    {
        printf("%d not a palindrome", c);
    }    
    else 
    {
        printf("a palindrome%d", c);
    }
    return 0;
}
